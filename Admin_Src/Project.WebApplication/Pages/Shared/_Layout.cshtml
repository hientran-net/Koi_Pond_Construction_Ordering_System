<body>

    <section id="container">
        <!--header start-->
        <header class="header white-bg">
            <div class="top-nav ">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Search">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="">
                                @if(User.Identity.IsAuthenticated)
                            {
                                <span class="username">@User.Identity.Name</span>
                            }
                            <b class="caret"></b>
                        </a>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <ul class="dropdown-menu extended logout" style="padding: 5px; text-align: center;">
                                <form method="get">
                                    <input type="submit"
                                           value="Log out"
                                           style="border-radius: 20px; background-color: lightblue; padding: 8px 20px; margin: 0; text-align: center; display: block; width: 100%;" />
                                </form>
                            </ul>
                        }
                </ul>
            </div>


        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                    <li>
                        <a href="/Index">
                            <i class="fa fa-dashboard"></i>
                            <span>Tổng Quan</span>
                        </a>
                    </li>

                    <!-- Quản lý dự án -->
                    <li>
                        <a href="/ProjectManagement">
                            <i class='bx bx-task'></i>
                            <span>Quản lý dự án</span>
                        </a>
                    </li>

                    <!-- Quản lý khách hàng -->
                    <li>
                        <a href="/CustomerManagement">
                            <i class='bx bxs-contact'></i>
                            <span>Quản lý khách hàng</span>
                        </a>
                    </li>

                    <!-- Quản lý đơn đặt hàng -->
                    <li>
                        <a href="/OderManagement">
                            <i class='bx bx-cart-alt'></i>
                            <span>Quản lý đơn đặt hàng</span>
                        </a>
                    </li>

                    <!-- Quản lý nhân viên -->
                    <li>
                        <a href="/EmployeeManagement">
                            <i class='bx bx-user-check'></i>
                            <span>Quản lý nhân viên</span>
                        </a>
                    </li>

                    <!-- Login -->
                    <li>
                        <a href="login.html">
                            <i class="fa fa-user"></i>
                            <span>Đăng Nhập</span>
                        </a>
                    </li>

                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        @RenderBody()
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                2013 &copy; FlatLab by VectorLab.
                <a href="#" class="go-top">
                    <i class="fa fa-angle-up"></i>
                </a>
            </div>
        </footer>
